{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "profiles": {
    "StudyPlatform.Web": {
      "commandName": "Project",
      "dotnetRunMessage": true,
      "launchBrowser": true,
      "applicationUrl": "https://localhost:7289/",
      "environmentVariable": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    }
  },
  "Identity": {
    "SignIn": {
      "RequireConfirmedAccount": false
    },
    "Password": {
      "RequireDigit": false,
      "RequireLowercase": false,
      "RequireUppercase": false,
      "RequiredLength":  4
    }
  }
}
